{
   "name": "ace",
   "version": "4.0.0",
   "description": "",
   "author": "Mohsen Hosseini",
   "license": "Commercial",
   "homepage": "https://bit.ly/35ciMLb",
   "main": "index.js",
   "scripts": {
      "css": "node-sass scss/ace.scss dist/css/ace.css && postcss --config build/postcss.config.js --replace \"dist/css/ace.css\"",
      "css-slim": "node-sass scss/ace-slim.scss dist/css/ace.css && postcss --config build/postcss.config.js --replace \"dist/css/ace-slim.css\"",
      "css-themes": "node-sass scss/ace-themes.scss dist/css/ace-themes.css && postcss --config build/postcss.config.js --replace \"dist/css/ace-themes.css\"",
      "css-main": "node-sass --output-style expanded --source-map true --source-map-contents true scss/ace.scss dist/css/ace.css && node-sass --output-style expanded --source-map true --source-map-contents true scss/ace-slim.scss dist/css/ace-slim.css && node-sass --output-style expanded --source-map true --source-map-contents true scss/ace-themes.scss dist/css/ace-themes.css",
      "css-rtl": "rm -r \"dist/css/rtl\" && mkdir \"dist/css/rtl\" && rtlcss \"dist/css/ace.css\" \"dist/css/rtl/ace.css\" && rtlcss \"dist/css/ace-slim.css\" \"dist/css/rtl/ace-slim.css\" && rtlcss \"dist/css/ace-themes.css\" \"dist/css/rtl/ace-themes.css\" && rtlcss \"node_modules/bootstrap/dist/css/bootstrap.css\" \"dist/css/rtl/bootstrap.css\"",
      "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/**/*.css\"",
      "css-all": "npm-run-all css-main css-prefix css-rtl",
      "css-min": "npm-run-all css-min-*",
      "css-min-main": "cleancss --format --source-map --source-map-inline-sources --output dist/css/ace.min.css dist/css/ace.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/ace-themes.min.css dist/css/ace-themes.css",
      "css-min-rtl": "cleancss --format --source-map --source-map-inline-sources --output dist/css/rtl/ace.min.css dist/css/rtl/ace.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/rtl/ace-themes.min.css dist/css/rtl/ace-themes.css && cleancss --format --source-map --source-map-inline-sources --output dist/css/rtl/bootstrap.min.css dist/css/rtl/bootstrap.css",
      "css-docs": "node-sass ../ace-docs/assets/scss/style.scss ../ace-docs/assets/style.css",
      "js": "rollup --config build/rollup.config.js --sourcemap",
      "js-copy-to-ng": "cp js/src/*.js ../ace-angular/projects/ng-ace-lib/src/js && cd ../ace-angular/projects/ng-ace-lib/src/js/ && rm aside.js general.js index.umd.js toaster.js wysiwyg.js",
      "js-min": "npm-run-all js-min-*",
      "js-min-main": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/ace.js.map,includeSources,url=ace.min.js.map\" --output dist/js/ace.min.js dist/js/ace.js",
      "js-min-demo": "uglifyjs --compress typeofs=false --mangle --comments \"/^!/\" --output app/browser/demo.min.js app/browser/demo.js",
      "js-lint": "eslint js/src/*.js",
      "js-test": "karma start js/tests/karma.conf.js",
      "release-html": "cross-env HTML=true node index.js",
      "demo-html": "cross-env HTML=true BUILD=dist MIN=true node index.js",
      "release-package": "node build/zip-package-builder.js",
      "demo-package": "cross-env PACKAGE=demo node build/zip-package-builder.js",
      "prepare-all": "npm-run-all css-all css-min js js-min",
      "release-build": "npm-run-all release-html css-docs generate-docs release-package",
      "release": "npm-run-all prepare-all js-copy-to-ng release-build && rm -r \"docs\" && rm -r \"html\"",
      "demo": "npm-run-all prepare-all demo-html demo-package && rm -r \"html\"",
      "generate-docs": "cross-env BUILD=true OUTPUT_DIR=../ace/docs MODULES_DIR=../ace ASSETS_DIR=. node ../ace-docs/index.js",
      "local": "node index.js"
   },
   "dependencies": {
      "@fortawesome/fontawesome-free": "^5.15.3",
      "@jaames/iro": "^5.5.1",
      "ag-grid-community": "^25.1.0",
      "aos": "^2.3.4",
      "autosize": "^4.0.2",
      "basictable": "^2.0.2",
      "bootbox": "^5.5.2",
      "bootstrap": "^4.6.0",
      "bootstrap-colorpicker": "^3.2.0",
      "bootstrap-duallistbox": "^3.0.9",
      "bootstrap-markdown": "^2.10.0",
      "bootstrap-maxlength": "^1.10.0",
      "bootstrap-select": "^1.13.18",
      "bootstrap-star-rating": "^4.0.6",
      "bootstrap-table": "^1.18.3",
      "bootstrap-tagsinput": "^0.7.1",
      "bootstrap-touchspin": "^4.3.0",
      "bootstrap-wysiwyg": "^2.0.1",
      "chart.js": "^2.9.4",
      "chartist": "^0.11.4",
      "chartjs-plugin-datalabels": "^0.7.0",
      "chosen-js": "^1.8.7",
      "code-prettify": "github:google/code-prettify#master",
      "color-themes-for-google-code-prettify": "^2.0.4",
      "datatables": "^1.10.18",
      "datatables.net-bs4": "^1.10.24",
      "datatables.net-buttons": "^1.7.0",
      "datatables.net-buttons-bs4": "^1.7.0",
      "datatables.net-colreorder-bs": "^1.5.3",
      "datatables.net-fixedheader": "^3.1.8",
      "datatables.net-responsive": "^2.2.7",
      "datatables.net-select": "^1.3.3",
      "dropzone": "^5.9.2",
      "eonasdan-bootstrap-datetimepicker": "^4.17.49",
      "es6-object-assign": "^1.1.0",
      "eva-icons": "^1.1.3",
      "flot": "^4.2.2",
      "free-jqgrid": "^4.15.5",
      "fullcalendar": "^5.6.0",
      "holderjs": "^2.9.9",
      "inputmask": "^5.0.5",
      "interactjs": "^1.10.11",
      "ion-rangeslider": "^2.3.1",
      "jam-icons": "^2.0.0",
      "jqtree": "^1.6.0",
      "jquery": "^3.6.0",
      "jquery-knob": "^1.2.11",
      "jquery-validation": "^1.19.3",
      "jquery.hotkeys": "^0.1.0",
      "lunr": "^2.3.9",
      "mark.js": "^8.11.1",
      "markdown": "^0.5.0",
      "moment": "^2.29.1",
      "node-waves": "^0.7.6",
      "nouislider": "^14.7.0",
      "photoswipe": "^4.1.3",
      "popper.js": "^1.16.1",
      "prism-themes": "^1.6.0",
      "prismjs": "^1.23.0",
      "select2": "^4.1.0-rc.0",
      "smartwizard": "^4.4.1",
      "sortablejs": "^1.13.0",
      "summernote": "^0.8.18",
      "sweetalert2": "^10.16.0",
      "tableexport.jquery.plugin": "^1.10.22",
      "tiny-date-picker": "^3.2.8",
      "typeahead.js": "^0.11.1"
   },
   "devDependencies": {
      "@babel/cli": "^7.13.14",
      "@babel/core": "^7.13.15",
      "@babel/preset-env": "^7.13.15",
      "archiver": "^5.3.0",
      "autoprefixer": "^10.2.5",
      "clean-css-cli": "^5.2.2",
      "cross-env": "^7.0.3",
      "csvjson": "^5.1.0",
      "eslint": "^7.24.0",
      "eslint-config-standard": "^16.0.2",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-promise": "^4.3.1",
      "eslint-plugin-standard": "^5.0.0",
      "express": "^4.17.1",
      "express-handlebars": "^5.3.0",
      "glob": "^7.1.6",
      "handlebars": "^4.7.7",
      "karma": "^6.3.2",
      "karma-chrome-launcher": "^3.1.0",
      "karma-qunit": "^4.1.2",
      "karma-viewport": "^1.0.8",
      "node-sass": "^5.0.0",
      "npm-run-all": "^4.1.5",
      "pkg-dir": "^5.0.0",
      "postcss": "^8.2.9",
      "postcss-cli": "^8.3.1",
      "postcss-loader": "^5.2.0",
