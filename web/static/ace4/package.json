      "qunit": "^2.14.1",
      "relative": "^3.0.2",
      "resolve": "^1.20.0",
      "resolve-pkg": "^2.0.0",
      "rollup": "^2.45.1",
      "rollup-plugin-babel": "^4.4.0",
      "rtlcss": "^3.1.2",
      "uglify-js": "^3.13.3",
      "xtend": "^4.0.2"
   }
}
